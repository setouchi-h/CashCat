# === Solana ===
SOLANA_PRIVATE_KEY=               # Wallet private key (Base64), required for live trading
HELIUS_API_KEY=                   # Helius RPC (recommended, free tier available)

# === Swap / Price APIs ===
JUPITER_API_KEY=                  # Jupiter API key (optional, required for api.jup.ag)
JUPITER_API_BASE_URL=             # Optional override. Default: api.jup.ag with key, otherwise lite-api.jup.ag

# === Agent Settings ===
PAPER_TRADE=true                  # true=paper trading / false=live trading
SCAN_INTERVAL_SECONDS=20          # Scan interval (seconds)
RUNTIME_INTENT_DIR=/tmp/cashcat-runtime/intents
RUNTIME_RESULT_DIR=/tmp/cashcat-runtime/results
RUNTIME_PROPOSAL_DIR=/tmp/cashcat-runtime/proposals
RUNTIME_VERDICT_DIR=/tmp/cashcat-runtime/verdicts
RUNTIME_MAX_INTENTS_PER_CYCLE=10
RUNTIME_MAX_PROPOSALS_PER_CYCLE=5
RUNTIME_KILL_SWITCH=false
RUNTIME_MAX_AMOUNT_LAMPORTS=50000000000
RUNTIME_MAX_SLIPPAGE_BPS=300
RUNTIME_ALLOWED_INPUT_MINTS=
RUNTIME_ALLOWED_OUTPUT_MINTS=
RUNTIME_AUTO_RUN_COMMAND=
RUNTIME_AUTO_RUN_CWD=
RUNTIME_COMMAND_TIMEOUT_SECONDS=120
RUNTIME_GATE_MIN_PNL_DELTA_PCT=0.2
RUNTIME_GATE_MIN_SHARPE_DELTA=0.05
RUNTIME_GATE_MAX_DRAWDOWN_DELTA_PCT=0
RUNTIME_GATE_MIN_TEST_PASS_RATE=0.98
LOG_LEVEL=info                    # debug | info | warn | error
