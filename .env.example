# === LLM Settings ===
LLM_PROVIDER=chatgpt-oauth       # chatgpt-oauth | anthropic
OPENAI_MODEL=gpt-5.2             # Model used by chatgpt-oauth
OPENAI_REASONING_EFFORT=medium   # none | low | medium | high | xhigh
OPENAI_API_KEY=                   # OpenAI API key (when not using OAuth)
ANTHROPIC_API_KEY=                # Required only when using anthropic

# === Solana ===
SOLANA_PRIVATE_KEY=               # Wallet private key (Base64), required for live trading
HELIUS_API_KEY=                   # Helius RPC (recommended, free tier available)

# === Data Sources ===
BIRDEYE_API_KEY=                  # Birdeye API (optional)

# === Agent Settings ===
PAPER_TRADE=true                  # true=paper trading / false=live trading
SCAN_INTERVAL_MINUTES=5           # Scan interval (minutes)
LOG_LEVEL=info                    # debug | info | warn | error
